/* pages/per/person_message/person_message.wxss */
.section{
  display: flex;
  flex-direction: column;
  justify-content: flex-end;

  position: absolute;
}
.jx_card{
  width: 100%;
  height: 295rpx;
  position: relative;
}
 .jxlogo{
  top: 47.5rpx;
  margin-left: 150rpx;
  width: 200rpx;
  height: 200rpx;
  float: left;
  position: absolute;

}.personalList{
  
  padding:0rpx 20rpx;
  width: 100%;
  height: 295rpx;
  position: relative;
}
.personalList view:before{
    color:#b02923;
    font-size:20px !important;
    font-weight:bold;
    padding-right: 10rpx;
    text-align:center;
    vertical-align:top;
}
.personalList view:after{
    content:'';
    height:10px;
    width:10px;
    display:block;
    border:1px solid #ccc;
    border-left-width:0;
    border-bottom-width:0;
    position:absolute;
    top:10px;
    right:6px;
    -webkit-transform:rotate(45deg);
       -moz-transform:rotate(45deg);
            transform:rotate(45deg);
}
.personalList view{
    display: block;
    line-height:70rpx;
    height:70rpx;
    border-bottom:1px solid #ddd;
}
button::after{ 
	border: none; 
}

button {
    margin-top: 20px;
    border-radius: 30px;
    width: 300px;
}
.btn1 {
  background: #457fca; 
}